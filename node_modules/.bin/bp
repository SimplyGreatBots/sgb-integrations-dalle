#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/@botpress+cli@0.5.0/node_modules/@botpress/cli/node_modules:/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/@botpress+cli@0.5.0/node_modules/@botpress/node_modules:/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/@botpress+cli@0.5.0/node_modules:/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/@botpress+cli@0.5.0/node_modules/@botpress/cli/node_modules:/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/@botpress+cli@0.5.0/node_modules/@botpress/node_modules:/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/@botpress+cli@0.5.0/node_modules:/mnt/c/Users/camer/OneDrive/Desktop/Simply Great Bots/Custom Integrations/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@botpress/cli/bin.js" "$@"
else
  exec node  "$basedir/../@botpress/cli/bin.js" "$@"
fi
